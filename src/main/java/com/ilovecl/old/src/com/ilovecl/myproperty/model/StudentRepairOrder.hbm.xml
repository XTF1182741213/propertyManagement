<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ilovecl.myproperty.model.StudentRepairOrder"
           table="StudentRepairOrder">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="increment"/>
        </id>
        <property name="studentId" type="java.lang.Integer">
            <column name="studentId"/>
        </property>
        <property name="repairOrderId" type="java.lang.Integer">
            <column name="repairOrderId"/>
        </property>
    </class>
</hibernate-mapping>
